<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<ThingDef ParentName="ResourceBase">
		<defName>O21_MechanoidPowerCell</defName>
		<label>mechanoid power cell</label>
		<description>Consumable cells of energy used by mechanoids and mechadroids to recharge.</description>
		<graphicData>
			<texPath>Mechadroids/Item/PowerCell</texPath>
			<graphicClass>Graphic_StackCount</graphicClass>
		</graphicData>
		<socialPropernessMatters>true</socialPropernessMatters>
		<statBases>
			<MarketValue>10</MarketValue>
			<Mass>0.025</Mass>
			<MaxHitPoints>50</MaxHitPoints>
			<Flammability>0.3</Flammability>
			<DeteriorationRate>2.0</DeteriorationRate>
		</statBases>
		<recipeMaker>
			<workSpeedStat>GeneralLaborSpeed</workSpeedStat>
			<effectWorking>Cook</effectWorking>
			<soundWorking>Recipe_Machining</soundWorking>
			<workAmount>800</workAmount>
			<targetCountAdjustment>5</targetCountAdjustment>
			<productCount>5</productCount>
			<workSkill>Crafting</workSkill>
			<recipeUsers>
				<li>FabricationBench</li>
				<li>O21_Mechadroid_Workbench</li>
			</recipeUsers>
            <researchPrerequisite>O21_Mechanoids_PowerCells</researchPrerequisite>
		</recipeMaker>
		<costList>
			<Chemfuel>2</Chemfuel>
            <Plasteel>3</Plasteel>
		</costList>
		<comps>
			<li Class="O21Toolbox.Needs.CompProperties_EnergySource">
				<isConsumable>true</isConsumable>
				<energyWhenConsumed>0.70</energyWhenConsumed>
			</li>
		</comps>
		<thingCategories>
			<li>Manufactured</li>
		</thingCategories>
	</ThingDef>
	
</Defs>